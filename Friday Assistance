import subprocess
import webbrowser
import pyttsx3
import os
import datetime

friday = pyttsx3.init()
user_command_count = 0
user_name = input("Please enter your name sir/ma'm: ")

def wishMe():   
    hour = int(datetime.datetime.now().hour)
    if hour>=0 and hour<12:
         friday.say("Good Morning!")

    elif hour>=12 and hour<18:
        friday.say("Good Afternoon!")

    else:
        friday.say("Good Evening!")

if __name__ == "__main__":
    wishMe()
friday.runAndWait()

friday.say(f"hi {user_name} this is friday and please type all alphabets in small alphabets thankyou")
friday.runAndWait()

while user_command_count < 1000:
    user_command = input(f"How can I help you {user_name}> ")

# These words will be said by friday
    if user_command == "hi":
        friday.say(f"hi {user_name} this is friday how can i help you")
        friday.runAndWait()
    if user_command == "what are you doing":
        friday.say(f"helping you {user_name}")
        friday.runAndWait()
    if user_command == "what is my name":
        friday.say(f"your name is {user_name}")
        friday.runAndWait()
    if user_command == "bad":
        friday.say(f"sorry {user_name} i will improve")
        friday.runAndWait()
    if user_command == "good":
        friday.say(f"thankyou {user_name} for giving good")
        friday.runAndWait()
    if user_command == "excelent":
        friday.say(f"very very very thankyou {user_name} for giving excelent")
        friday.runAndWait()
    if user_command == "thankyou for opening":
        friday.say(f"ok {user_name} your welcome")
        friday.runAndWait()
    if user_command == "thankyou":
        friday.say(f"ok {user_name} your welcome")
        friday.runAndWait()

# These are url are opened by google
    if user_command == "open google":
        friday.say(f"ok {user_name} opening google")
        webbrowser.open('www.google.com')
        friday.runAndWait()
